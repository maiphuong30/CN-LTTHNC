<div class="table-responsive">
<table class="table table-striped table-sm">
    <thead>
        <tr>
            <th>Tên danh mục</th>
            <th>Giá</th>
            <th>Header</th>
            <th>Sửa</th>
            <th>Xóa</th>
        </tr>
    </thead>
    <tbody>
        <% danhsach.forEach(function(dm){ %>
            <tr>
                <td>
                    <%- dm.Title %>
                </td>
                <td>bla bla</td>
                <td>bla bla</td>
                <td><a href="danhmuc/edit/<%- dm._id %>">
                    <i class="bi bi-pencil-square"  style="color:blue;"></i></a></td>
                <td><a onclick="return confirm('Bạn có chắc chắn muốn xóa <%- dm.Title %>?')" href="danhmuc/delete/<%- dm._id %>"><i class="bi bi-trash" style="color: red;"></i></a></td>
            </tr>
            <% }); %>
    </tbody>
</table>
</div>