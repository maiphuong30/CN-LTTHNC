<div class="album py-5 bg-light">
  
  <section id="portfolio" class="portfolio">
    <div class="container">

      <div class="section-title">
        <h2>Portfolio</h2>
        <h3>Check our <span>Portfolio</span></h3>
        <p>Ut possimus qui ut temporibus culpa velit eveniet modi omnis est adipisci expedita at voluptas atque vitae autem.</p>
      </div>
      
      <div class="row">
        <div class="col-lg-12 d-flex justify-content-center">
          <ul id="portfolio-flters">
            <li data-filter="*" class="filter-active">All</li>
            <li data-filter=".filter-app">Product 1</li>
            <li data-filter=".filter-card">Product 2</li>
            <li data-filter=".filter-web">Product 3</li>
          </ul>
        </div>
      </div>
      <div class="row portfolio-container">
        <% danhsach.forEach(function(sp){ %>
          
        <div class="col-lg-4 col-md-6 portfolio-item filter-app">
          <img src="./images/<%- sp.Image %>" class="img-fluid" alt="">
          <div class="portfolio-info">
            <h4>
              <%- sp.Name %>
            </h4>
            <a href="./images/<%- sp.Image %>" data-gall="portfolioGallery" class="venobox preview-link" title="<%- sp.Name %>"><i class="bx bx-plus"></i></a>
            <a href="/detail/<%- sp._id %>" class="details-link" title="More Details"><i class="bx bx-link"></i></a>
          </div>
        </div>

          <% }); %>  
      </div>
      <%- include('phantrang'); %>
      
    </div>
  </section><!-- End Portfolio Section -->
